               await emailjs.send(\"service_9tk1mlb\",\"template_d66po7d\",{\n                        to_name: \"Agnel Fernando\",\n                        from_name: this.from_name,\n                        message: this.message,\n                        reply_to: this.email,\n                    },'v3bOgNALuXq3n2HLr').then(() => {\n                        SwalEmail.fire({\n                            icon: 'success',\n                            title: i18n.t('Email Sent!'),\n                            text: i18n.t('Thanks'),\n                        })\n                    }).finally(()=>{\n                        this.isSending = false\n                    })\n                } catch (error) {\n                    console.log(error)\n                    SwalEmail.fire({\n                        icon: 'error',\n                        title: i18n.t('Email Not Sent!'),\n                        text: error\n                    })\n                }\n            }\n        }\n    }\n}\n</script>\n"],"mappings":"AA4CA,OAAAA,OAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AAEA,MAAAC,SAAA,GAAAF,IAAA,CAAAG,KAAA;EACAC,WAAA;IACAC,KAAA;IACAC,KAAA;IACAC,OAAA;IACAC,aAAA;EACA;EACAC,cAAA;AACA;AAEA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,UAAA;MACA,KAAAF,SAAA;MACA,SAAAH,SAAA,gBAAAC,KAAA,gBAAAC,OAAA;QACA;UACA,MAAAd,OAAA,CAAAkB,IAAA;YACAC,OAAA;YACAP,SAAA,OAAAA,SAAA;YACAE,OAAA,OAAAA,OAAA;YACAM,QAAA,OAAAP;UACA,wBAAAQ,IAAA;YACAlB,SAAA,CAAAmB,IAAA;cACAC,IAAA;cACAhB,KAAA,EAAAL,IAAA,CAAAsB,CAAA;cACAC,IAAA,EAAAvB,IAAA,CAAAsB,CAAA;YACA;UACA,GAAAE,OAAA;YACA,KAAAX,SAAA;UACA;QACA,SAAAY,KAAA;UACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;UACAxB,SAAA,CAAAmB,IAAA;YACAC,IAAA;YACAhB,KAAA,EAAAL,IAAA,CAAAsB,CAAA;YACAC,IAAA,EAAAE;UACA;QACA;MACA;IACA;EACA;AACA"}]}